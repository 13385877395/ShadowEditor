    "description": "3D Editor with three.js.",
    "dependencies": {
        "classnames": "^2.2.6",
        "prop-types": "^15.7.2",
        "rollup-plugin-postcss": "^2.0.3",
        "rollup-plugin-replace": "^2.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.4.4",
        "@babel/preset-react": "^7.0.0",
